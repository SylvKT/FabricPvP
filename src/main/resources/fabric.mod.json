{
  "schemaVersion": 1,
  "id": "fabricpvp",
  "version": "${version}",

  "name": "FabricPvP",
  "description": "A PvP Minecraft client made for use with the Legacy Fabric Minecraft mod loader.",
  "authors": [
    "TehcJS",
    "hydos"
  ],
  "contact": {
    "sources": "https://github.com/TehcJS/FabricPvP"
  },

  "license": "Apache 2",
  "icon": "assets/fabricpvp/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.tehcjs.fabricpvp.FabricPvP"
    ]
  },
  "mixins": [
    "fabricpvp.mixins.json",
    "fabricpvp.chunk_optimizations.mixins.json"
  ],
  "accessWidener": "fabricpvp.aw",
  "depends": {
    "fabricloader": ">=0.7.4"
  },
  "breaks": {
    "optifabric": "*"
  }
}
